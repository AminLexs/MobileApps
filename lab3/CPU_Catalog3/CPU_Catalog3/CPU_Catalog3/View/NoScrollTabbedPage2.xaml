            xmlns:local="clr-namespace:CPU_Catalog3.View" xmlns:local2="clr-namespace:CPU_Catalog3" 
            x:Class="CPU_Catalog3.View.NoScrollTabbedPage2">

    <TabbedPage.Children>
        <local:DetailedPage IconImageSource="{local2:ImageResource CPU_Catalog3.Images.ProfileIcon.png}"/>
        <local:EditPage IconImageSource="{local2:ImageResource CPU_Catalog3.Images.Edit.png}"/>
        <local:ImageGalleryPage IconImageSource="{local2:ImageResource CPU_Catalog3.Images.Picture.png}"/>
        <local:VideoPage  IconImageSource="{local2:ImageResource CPU_Catalog3.Images.Video.png}"/>
    </TabbedPage.Children>
</TabbedPage>